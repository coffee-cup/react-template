{
  "name": "react-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --hot --inline --progress",
    "dash": "cross-env NODE_ENV=dev webpack-dashboard -- webpack-dev-server --hot --inline --progress",
    "build": "cross-env NODE_ENV=production webpack -p",
    "copy": "cp -r public/* build; cp public/index.html build/200.html",
    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "postbuild": "npm run copy",
    "predeploy": "npm run build",
    "deploy": "surge build/ https://coffee-cup-react-template.surge.sh"
  },
  "author": [
    "Jake Runzer <jakerunzer@gmail.com> (https://jakerunzer.xyz)"
  ],
  "license": "MIT",
